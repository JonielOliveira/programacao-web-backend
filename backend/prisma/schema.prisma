generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  username  String  @id
  nome      String
  email     String  @unique
  tipo      String  @db.Char(1)  // '0' = Admin, '1' = Comum
  status    String  @db.Char(1)  // 'A', 'I', 'B'
}
